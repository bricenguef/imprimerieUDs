<script type="text/javascript">

    function NoLastName(ele) {

        var txtBox = document.getElementById('_ctl0_ContentPlaceHolder1_formApplicant_qName_txtLastName');
         if (ele.checked) {
            if (txtBox != null) {
                txtBox.value = '';
                txtBox.disabled = true;
            }
        }
        else
            txtBox.disabled = false;
       
        return true;
    }

    function NoFirstName(ele) {

        var txtBox = document.getElementById('_ctl0_ContentPlaceHolder1_formApplicant_qName_txtFirstName');
        if (ele.checked) {
            if (txtBox != null) {
                txtBox.value = '';
                txtBox.disabled = true;
            }
        }
        else
            txtBox.disabled = false;
        return true;
    }

    function NoMiddleName(ele) {

        var txtBox = document.getElementById('_ctl0_ContentPlaceHolder1_formApplicant_qName_txtMiddleName');
        if (ele.checked) {
            if (txtBox != null) {
                txtBox.value = '';
                txtBox.disabled = true;
            }
        }
        else
            txtBox.disabled = false;
        return true;
    }
</script>


